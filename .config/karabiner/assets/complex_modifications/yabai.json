          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command", "fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --warp east"
            }
          ]
        }
      ]
    },
