    {
      "description": "Press Caps lock + B/F/S to change layout of the current desktop to BSP/Float/Stack layout.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m config layout bsp"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m config layout float"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m config layout stack"
            }
          ]
        }
      ]
    },
    {
      "description": "Press Caps lock + X/Y to flip the tree of the current desktop",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "x",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --mirror x-axis"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --mirror y-axis"
            }
          ]
        }
      ]
    },
    {
      "description": "Press Caps lock + R to rotate the tree of the current desktop. Press Caps lock + Fn + R to toggle window split type",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --rotate 90"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command", "Fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --toggle split"
            }
          ]
        }
      ]
    },
    {
      "description": "Press Caps lock + Z to make the current window fullscreen. Press Capslock + Fn + Z to zoom the current window out to its parent node",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --toggle zoom-fullscreen"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command", "fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --toggle zoom-parent"
            }
          ]
        }
      ]
    },
    {
      "description": "Press Caps lock + 0 to make all windows occupy the same area",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "0",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --balance"
            }
          ]
        }
      ]
    },
    {
      "description": "Press Caps lock + Fn + F to make the current window float or unfloat",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command", "fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --toggle float"
            }
          ]
        }
      ]
    },
    {
      "description": "Press Caps lock + G to toggle current space padding and gap between windows",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --toggle padding && /usr/local/bin/yabai -m space --toggle gap"
            }
          ]
        }
      ]
    }
  ]
}