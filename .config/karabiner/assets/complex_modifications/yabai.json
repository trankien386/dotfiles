{
  "title": "Yabai Manipulation",
  "rules": [
    {
      "description": "Press Fn + h/j/k/l/s to set insertion point in the current window.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --insert west"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --insert south"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --insert north"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --insert east"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --insert stack"
            }
          ]
        }
      ]
    },
    {
      "description": "Press Option + h/j/k/l to focus the current window. Press Caps lock + N/P to cycle between stacked windows",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["left_alt"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus west"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["left_alt"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus south"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["left_alt"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus north"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["left_alt"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus east"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus stack.next"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus stack.prev"
            }
          ]
        }
      ]
    },
    {
      "description": "Press Caps lock + h/j/k/l to swap position of the current window and the given window.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --swap west"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --swap south"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --swap north"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --swap east"
            }
          ]
        }
      ]
    },
    {
      "description": "Press Caps lock + Fn + h/j/k/l to re-insert the current window, splitting the given window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command", "fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --warp west"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command", "fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --warp south"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command", "fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --warp north"
