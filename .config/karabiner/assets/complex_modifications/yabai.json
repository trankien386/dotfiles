{
  "title": "Yabai",
  "rules": [
    {
      "description": "Press Caps lock + Fn + 1-9 to send the current window to the given desktop",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command", "fn"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --space 1"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": ["left_shift", "left_control", "left_alt", "left_command", "fn"]
            }
          },
